{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["import", "jsx-a11y", "react", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
